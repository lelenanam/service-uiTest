{
  "name": "scope-service-client",
  "version": "0.1.0",
  "private": true,
  "description": "UI for Scope as a service",
  "main": "src/main.jsx",
  "scripts": {
    "start": "node server.js",
    "start-production": "NODE_ENV=production node server.js",
    "lint": "eslint src server.js --ext=jsx --ext=js",
    "build": "webpack --bail --config webpack.local.config.js",
    "build-internal": "webpack --bail --config webpack.production.config.js",
    "build-external": "EXTERNAL=true webpack --bail --config webpack.production.config.js",
    "test": "jest",
    "analyze": "ANALYZE_BUNDLE=true npm run build-external",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "clean": "rm build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/weaveworks/service.git"
  },
  "author": "Weaveworks",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/weaveworks/service/issues"
  },
  "homepage": "https://github.com/weaveworks/service",
  "dependencies": {
    "babel-polyfill": "6.20.0",
    "classnames": "2.2.5",
    "d3": "3.5.16",
    "debug": "2.4.5",
    "draft-js": "^0.9.1",
    "font-awesome": "4.7.0",
    "font-awesome-webpack": "0.0.4",
    "git-url-parse": "^6.2.2",
    "lodash": "4.17.2",
    "moment": "2.17.1",
    "promjs": "^0.3.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.1",
    "react-redux-form": "^1.11.0",
    "react-router": "3.0.0",
    "redux": "^3.5.2",
    "reselect": "^2.5.4",
    "rickshaw": "1.6.0",
    "weave-scope": "https://s3.amazonaws.com/weaveworks-js-modules/weave-scope/1f078a8a/weave-scope.tgz",
    "weaveworks-ui-components": "git+https://github.com/weaveworks/ui-components.git#v0.1.23"
  },
  "devDependencies": {
    "autoprefixer": "6.5.4",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "clean-webpack-plugin": "0.1.14",
    "css-loader": "0.26.1",
    "enzyme": "^2.7.0",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jasmine": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "jest-cli": "18.0.0",
    "less": "2.7.2",
    "postcss-loader": "1.2.0",
    "react-addons-test-utils": "15.4.1",
    "reqwest": "2.0.5",
    "sass-loader": "^4.1.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-bundle-analyzer": "^2.3.0",
    "xhr2": "0.1.3"
  },
  "optionalDependencies": {
    "body-parser": "1.13.3",
    "coveralls": "2.11.3",
    "express": "4.13.3",
    "http-proxy-middleware": "0.17.3",
    "socks": "^1.1.10",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.2"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "engines": {
    "node": "6.9.0"
  }
}
